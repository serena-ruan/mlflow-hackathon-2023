  "scripts": {
    "build": "./scripts/build.sh",
    "build:css": "node_modules/.bin/fragments build src/css",
    "build-storybook": "yarn storybook:css && build-storybook",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path ./",
    "chromatic": "chromatic",
    "clean:css": "yarn build:css && node_modules/.bin/purgecss --config purgecss.config.js --out public/css",
    "deploy": "./scripts/publish.sh",
    "generate:codesample": "./scripts/generate-code-sample/generate.sh",
    "lint": "eslint src --ext .ts,.tsx",
    "prepare": "husky install",
    "preversion": "./scripts/preversion.sh",
    "postversion": "./scripts/postversion.sh",
    "version": "run-s version-script changelog",
    "version-beta": "./scripts/version-beta.sh",
    "version-script": "./scripts/version.sh",
    "storybook": "yarn storybook:css && start-storybook -p 6006",
    "storybook:css": "yarn build:css && node_modules/.bin/purgecss --config purgecss.config.js --out .storybook",
    "test": "jest --watch",
    "test:ci": "jest test --runInBand --silent"
  },
